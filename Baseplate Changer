-- Scripts:

local function MMIZ_fake_script() -- OPENSCRIPT.script that closes startscreen opens script 
	local script = Instance.new('LocalScript', OPENSCRIPT)

	local playFrame = script.Parent.Parent -- Reference to PlayFrame
	local mainFrame = playFrame.Parent:WaitForChild("MainFrame") -- Reference to MainFrame
	local openClose = playFrame.Parent:WaitForChild("OpenClose") -- Reference to OpenClose button
	
	script.Parent.MouseButton1Click:Connect(function()
		playFrame.Visible = false   -- Close PlayFrame
		mainFrame.Visible = true     -- Open MainFrame
		openClose.Visible = true     -- Show OpenClose button
	end)
	
end
coroutine.wrap(MMIZ_fake_script)()
local function CXNAZT_fake_script() -- BASEPLATEMETAL.BASEPLATE CHANGER 
	local script = Instance.new('Script', BASEPLATEMETAL)

	local baseplate = game.Workspace.Baseplate
	local originalMaterial = baseplate.Material
	local isMetal = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if isMetal then
			baseplate.Material = originalMaterial
			isMetal = false
		else
			baseplate.Material = "Metal"
			isMetal = true
		end
	end)
end
coroutine.wrap(CXNAZT_fake_script)()
local function KBMSZH_fake_script() -- BASEPLATEBRICK.BASEPLATE CHANGER 
	local script = Instance.new('Script', BASEPLATEBRICK)

	local baseplate = game.Workspace.Baseplate
	local originalMaterial = baseplate.Material
	local Brick = false
	
