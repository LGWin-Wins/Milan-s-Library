local function MQPBBRP_fake_script() -- OpenClose.Open and close script 
	local script = Instance.new('LocalScript', OpenClose)

	local openCloseButton = script.Parent
	local mainFrame = openCloseButton.Parent:WaitForChild("MainFrame")
	
	openCloseButton.MouseButton1Click:Connect(function()
		mainFrame.Visible = not mainFrame.Visible  -- Toggle visibility
	end)
	
end
