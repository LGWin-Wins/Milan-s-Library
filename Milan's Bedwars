-- Scripts:

local function ZWNX_fake_script() -- ScrollingFrame.LocalScript 
	local script = Instance.new('LocalScript', ScrollingFrame)

	local Gui = script.Parent
	
	local function onKeyPress(input)
		if input.KeyCode == Enum.KeyCode.RightShift then
			if Gui.Visible == true then
				Gui.Visible = false
			else
				Gui.Visible = true
			end
		end
	end
	
	game:GetService("UserInputService").InputBegan:Connect(onKeyPress)
	
end
coroutine.wrap(ZWNX_fake_script)()
local function BGWTC_fake_script() -- AUTOCLICKER.AUTOCLICKER SCRIPT 
	local script = Instance.new('LocalScript', AUTOCLICKER)

	local UserInputService = game:GetService("UserInputService")
	local Players = game:GetService("Players")
	local button = script.Parent
	local autoclickerEnabled = false
	local clickConnection = nil
	
	local function autoclickerLoop()
		while autoclickerEnabled do
			UserInputService:Click()
			task.wait(0.05) -- 20 CPS (1/0.05 = 20)
		end
	end
	
	button.MouseButton1Click:Connect(function()
		autoclickerEnabled = not autoclickerEnabled
	
		if autoclickerEnabled then
			-- Start autoclicker loop
			clickConnection = task.spawn(autoclickerLoop)
	
			-- Handle new players
			Players.PlayerAdded:Connect(function(newPlayer)
				if autoclickerEnabled then
					newPlayer.CharacterAdded:Connect(function(character)
						-- Auto-clicker logic for new players
					end)
				end
			end)
		else
			-- Stop autoclicker
			if clickConnection then
				task.cancel(clickConnection)
				clickConnection = nil
			end
		end
	end)
	
end
coroutine.wrap(BGWTC_fake_script)()
