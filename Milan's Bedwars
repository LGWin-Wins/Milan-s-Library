-- Scripts:

local function YUYBPQJ_fake_script() -- ScrollingFrame1.rightshift to open 
	local script = Instance.new('LocalScript', ScrollingFrame1)

	local Gui = script.Parent
	
	local function onKeyPress(input)
		if input.KeyCode == Enum.KeyCode.RightShift then
			if Gui.Visible == true then
				Gui.Visible = false
			else
				Gui.Visible = true
			end
		end
	end
	
	game:GetService("UserInputService").InputBegan:Connect(onKeyPress)
	
end
coroutine.wrap(YUYBPQJ_fake_script)()
local function HVJXMD_fake_script() -- ScrollingFrame1.DRAG SCRIPT 
	local script = Instance.new('LocalScript', ScrollingFrame1)

	local scrollingFrame = script.Parent
	local mouse = game.Players.LocalPlayer:GetMouse()
	local dragging = false
	local startPos
	
	scrollingFrame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			startPos = Vector2.new(mouse.X, mouse.Y)
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	mouse.Move:Connect(function()
		if dragging then
			local currentPos = Vector2.new(mouse.X, mouse.Y)
			local delta = currentPos - startPos
			scrollingFrame.Position += UDim2.new(0, delta.X, 0, delta.Y)
			startPos = currentPos
		end
	end)
	
end
coroutine.wrap(HVJXMD_fake_script)()
local function FSWNONV_fake_script() -- infjump.Neon Red 
	local script = Instance.new('LocalScript', infjump)

	local button = script.Parent
	local stroke = Instance.new("UIStroke")
	stroke.Color = Color3.fromRGB(255, 0, 0)  -- Red color
	stroke.Thickness = 3
	stroke.Transparency = 0.3
	stroke.Parent = button
	
end
coroutine.wrap(FSWNONV_fake_script)()
local function DBINJ_fake_script() -- infjump.InfJumpCombined 
	local script = Instance.new('LocalScript', infjump)

	local button = script.Parent
	local player = game.Players.LocalPlayer
	local UIS = game:GetService("UserInputService")
	local infJumpEnabled = false  -- Default off
	
	-- Toggle infinite jump when button is clicked
	button.MouseButton1Click:Connect(function()
		infJumpEnabled = not infJumpEnabled
		button.Text = infJumpEnabled and "Infinite Jump: ON" or "Infinite Jump: OFF"
	end)
	
	-- Infinite jump logic
	UIS.JumpRequest:Connect(function()
		if infJumpEnabled and player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
			player.Character:FindFirstChildOfClass("Humanoid"):ChangeState(Enum.HumanoidStateType.Jumping)
		end
	end)
end
coroutine.wrap(DBINJ_fake_script)()
