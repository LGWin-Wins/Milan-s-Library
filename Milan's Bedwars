-- Scripts:

local function EQAVKC_fake_script() -- ScrollingFrame.LocalScript 
	local script = Instance.new('LocalScript', ScrollingFrame)

	local Gui = script.Parent
	
	local function onKeyPress(input)
		if input.KeyCode == Enum.KeyCode.RightShift then
			if Gui.Visible == true then
				Gui.Visible = false
			else
				Gui.Visible = true
			end
		end
	end
	
	game:GetService("UserInputService").InputBegan:Connect(onKeyPress)
	
end
coroutine.wrap(EQAVKC_fake_script)()
local function FBXFB_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local UserInputService = game:GetService("UserInputService")
	local Players = game:GetService("Players")
	local LocalPlayer = Players.LocalPlayer
	local Mouse = LocalPlayer:GetMouse()
	
	local autoclickerEnabled = false
	local clickInterval = 0.05 -- 20 CPS = 0.05 seconds per click
	
	local function startAutoClicker()
		while autoclickerEnabled and Mouse.Target do
			Mouse:Click()
			task.wait(clickInterval)
		end
	end
	
	local function onCharacterAdded(character)
		local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
	
		UserInputService.InputBegan:Connect(function(input, gameProcessed)
			if not gameProcessed and input.KeyCode == Enum.KeyCode.LeftShift then
				autoclickerEnabled = not autoclickerEnabled
				if autoclickerEnabled then
					startAutoClicker()
				end
			end
		end)
	end
	
	LocalPlayer.CharacterAdded:Connect(onCharacterAdded)
	if LocalPlayer.Character then
		onCharacterAdded(LocalPlayer.Character)
	end
	
end
coroutine.wrap(FBXFB_fake_script)()
local function ZVED_fake_script() -- ScrollingFrame_2.LocalScript 
