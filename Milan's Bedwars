	end
	
	game:GetService("UserInputService").InputBegan:Connect(onKeyPress)
	
end
coroutine.wrap(ZVED_fake_script)()
local function YLZRNP_fake_script() -- esptext.PlayerESP 
	local script = Instance.new('LocalScript', esptext)

	local Players = game:GetService("Players")
	local scrollingFrame = script.Parent
	local template = scrollingFrame:FindFirstChild("esp text")
	
	local function updateESP()
	    -- Clear existing entries
	    for _, child in scrollingFrame:GetChildren() do
	        if child:IsA("TextButton") and child ~= template then
	            child:Destroy()
	        end
	    end
	
	    -- Get all players
	    for _, player in Players:GetPlayers() do
	        local character = player.Character or player.CharacterAdded:Wait()
	        local humanoid = character:FindFirstChild("Humanoid")
	        
	        if humanoid and humanoid.Health > 0 then
	            local newEntry = template:Clone()
	            newEntry.Text = string.format("%s: %.1f, %.1f, %.1f", 
	                player.Name, 
	                character:GetPivot().Position.X,
	                character:GetPivot().Position.Y,
	                character:GetPivot().Position.Z
	            )
	            newEntry.Visible = true
	            newEntry.Parent = scrollingFrame
	        end
	    end
	end
	
	-- Initial update
	updateESP()
	
	-- Update every 5 seconds
	while true do
	    task.wait(5)
	    updateESP()
	end
	
	
end
coroutine.wrap(YLZRNP_fake_script)()
