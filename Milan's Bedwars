local function NKAMZ_fake_script() -- Open.Open 
	local script = Instance.new('LocalScript', Open)

	local ScrollingFrame1 = script.Parent.Parent.ScrollingFrame1
	
	script.Parent.MouseButton1Click:Connect(function()
		ScrollingFrame1.Visible = true
	end)
	local ScrollingFrame2 = script.Parent.Parent.ScrollingFrame2
	
	script.Parent.MouseButton1Click:Connect(function()
		ScrollingFrame2.Visible = true
	end)
	local ScrollingFrame3 = script.Parent.Parent.ScrollingFrame3
	
	script.Parent.MouseButton1Click:Connect(function()
		ScrollingFrame3.Visible = true
	end)
end
coroutine.wrap(NKAMZ_fake_script)()
